            @section styles{
                <style>
                    .k-grid tr .checkbox-align {
                        text-align: center;
                        vertical-align: middle;
                    }

                    .k-header {
                        text-align: center;
                    }

                    .product-photo {
                        display: inline-block;
                        width: 32px;
                        height: 32px;
                        border-radius: 50%;
                        background-size: 32px 35px;
                        background-position: center center;
